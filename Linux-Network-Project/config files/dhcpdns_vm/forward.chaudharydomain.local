@       IN      NS      ns1.chaudharydomain.local.

; DNS Server
ns1     IN      A       10.20.20.10

; Router
router  IN      A       10.20.20.1

; DHCP/DNS
dhcpdns IN      A       10.20.20.10

; Client
client  IN      A       10.20.20.50

; Web server in DMZ
web     IN      A       10.30.30.10
